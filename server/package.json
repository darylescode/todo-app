{
  "name": "server",
  "version": "1.0.0",
  "main": "src/server.js",
  "nodemonConfig": {
    "exec": "concurrently \"npx tsc --watch\" \"ts-node -r tsconfig-paths/register src/server.ts\""
  },
  "scripts": {
    "build": "npx tsc --project tsconfig.json && npx tsc-alias -p tsconfig.json",
    "start": "npx ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/server.ts",
    "codegen": "kysely-codegen --dialect mysql --out-file ./src/types/schema.type.ts",
    "migrations": "ts-node ./src/database/migrator.database.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.6",
    "concurrently": "^9.1.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "kysely-migration-cli": "^0.4.2",
    "mysql2": "^3.12.0",
    "server": "file:"
  }
}
